{
	"compilerOptions": {
		"skipLibCheck": true,
		"module": "es6",
		"target": "ES5",
		"jsx": "react-jsx",
		"allowJs": false,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"alwaysStrict": true,
		"baseUrl": "src",
		"isolatedModules": true
	}
}
