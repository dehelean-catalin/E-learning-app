{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": false,
		"module": "es6",
		"target": "es6",
		"jsx": "react-jsx",
		"allowJs": false,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"alwaysStrict": true
	}
}
