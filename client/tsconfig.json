{
	"compilerOptions": {
		"module": "es6",
		"target": "ES5",
		"jsx": "react-jsx",
		"allowJs": false,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"alwaysStrict": true,
		"baseUrl": "src",
		"paths": {
			"react-icons/*": ["node_modules/react-icons/*"]
		},
		"isolatedModules": true,
		"skipLibCheck": true
	}
}
